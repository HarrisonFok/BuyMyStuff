{
  "name": "buymystuff",
  "version": "1.0.0",
  "description": "BuyMyStuff app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "chatapp": "PORT=3001 npm start --prefix chatapp",
    "dev": "concurrently \"npm run chatapp\" \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HarrisonFok/BuyMyStuff.git"
  },
  "author": "Harrison Fok",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HarrisonFok/BuyMyStuff/issues"
  },
  "homepage": "https://github.com/HarrisonFok/BuyMyStuff#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
